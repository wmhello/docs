#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../@types/webpack-dev-middleware/node_modules/acorn/bin/acorn" "$@"
  ret=$?
else 
  node  "$basedir/../@types/webpack-dev-middleware/node_modules/acorn/bin/acorn" "$@"
  ret=$?
fi
exit $ret
